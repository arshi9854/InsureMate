    "        title_font_size=20,\n",
    "        showlegend=False,\n",
    "        height=600,\n",
    "        template=\"plotly_white\"\n",
    "    )\n",
    "    \n",
    "    return fig\n",
    "\n",
    "# Display the enhanced plot\n",
    "enhanced_dist_fig = create_enhanced_distribution_plot(df)\n",
    "enhanced_dist_fig.show()"
