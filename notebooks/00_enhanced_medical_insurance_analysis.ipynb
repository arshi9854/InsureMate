  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Enhanced smoking analysis with interactive plots\n",
    "def create_smoking_analysis(df):\n",
    "    fig = make_subplots(\n",
    "        rows=2, cols=2,\n",
    "        subplot_titles=('üí∞ Charges by Smoking Status', 'üìä Smoking Distribution',\n",
    "                       '‚öñÔ∏è BMI vs Charges (by Smoking)', 'üë• Age vs Charges (by Smoking)'),\n",
    "        specs=[[{\"type\": \"box\"}, {\"type\": \"pie\"}],\n",
    "               [{\"type\": \"scatter\"}, {\"type\": \"scatter\"}]]\n",
    "    )\n",
    "    \n",
    "    # Box plot for charges by smoking status\n",
    "    for smoking_status in ['no', 'yes']:\n",
    "        data = df[df['smoker'] == smoking_status]['charges']\n",
    "        color = '#4ECDC4' if smoking_status == 'no' else '#FF6B6B'\n",
    "        fig.add_trace(\n",
    "            go.Box(y=data, name=f'Smoker: {smoking_status.title()}',\n",
    "                  marker_color=color, boxmean=True),\n",
    "            row=1, col=1\n",
    "        )\n",
    "    \n",
    "    # Pie chart for smoking distribution\n",
    "    smoking_counts = df['smoker'].value_counts()\n",
    "    fig.add_trace(\n",
    "        go.Pie(labels=['Non-Smoker', 'Smoker'], values=smoking_counts.values,\n",
    "              marker_colors=['#4ECDC4', '#FF6B6B'],\n",
    "              hovertemplate='<b>%{label}</b><br>Count: %{value}<br>Percentage: %{percent}<extra></extra>'),\n",
    "        row=1, col=2\n",
    "    )\n",
    "    \n",
    "    # BMI vs Charges scatter plot\n",
    "    for smoking_status in ['no', 'yes']:\n",
    "        data = df[df['smoker'] == smoking_status]\n",
    "        color = '#4ECDC4' if smoking_status == 'no' else '#FF6B6B'\n",
    "        fig.add_trace(\n",
    "            go.Scatter(x=data['bmi'], y=data['charges'],\n",
    "                      mode='markers', name=f'Smoker: {smoking_status.title()}',\n",
    "                      marker=dict(color=color, size=6, opacity=0.7),\n",
    "                      hovertemplate='<b>BMI:</b> %{x}<br><b>Charges:</b> $%{y}<extra></extra>'),\n",
    "            row=2, col=1\n",
    "        )\n",
    "    \n",
    "    # Age vs Charges scatter plot\n",
    "    for smoking_status in ['no', 'yes']:\n",
    "        data = df[df['smoker'] == smoking_status]\n",
    "        color = '#4ECDC4' if smoking_status == 'no' else '#FF6B6B'\n",
    "        fig.add_trace(\n",
    "            go.Scatter(x=data['age'], y=data['charges'],\n",
    "                      mode='markers', name=f'Smoker: {smoking_status.title()}',\n",
    "                      marker=dict(color=color, size=6, opacity=0.7),\n",
    "                      hovertemplate='<b>Age:</b> %{x}<br><b>Charges:</b> $%{y}<extra></extra>'),\n",
    "            row=2, col=2\n",
    "        )\n",
    "    \n",
    "    fig.update_layout(\n",
    "        title_text=\"üö¨ Comprehensive Smoking Impact Analysis\",\n",
    "        title_x=0.5,\n",
    "        title_font_size=20,\n",
    "        height=800,\n",
    "        template=\"plotly_white\",\n",
    "        showlegend=True\n",
    "    )\n",
    "    \n",
    "    return fig\n",
    "\n",
    "# Display smoking analysis\n",
    "smoking_fig = create_smoking_analysis(df)\n",
    "smoking_fig.show()"
   ]
