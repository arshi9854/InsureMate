    "# Enhanced distribution plot with modern styling\n",
    "def create_enhanced_distribution_plot(df):\n",
    "    fig = make_subplots(\n",
    "        rows=2, cols=2,\n",
    "        subplot_titles=('üìä Charges Distribution', 'üë• Age Distribution', \n",
    "                       '‚öñÔ∏è BMI Distribution', 'üë∂ Children Distribution'),\n",
    "        specs=[[{\"secondary_y\": False}, {\"secondary_y\": False}],\n",
    "               [{\"secondary_y\": False}, {\"secondary_y\": False}]]\n",
    "    )\n",
    "    \n",
    "    # Charges distribution\n",
    "    fig.add_trace(\n",
    "        go.Histogram(x=df['charges'], nbinsx=30, name='Charges', \n",
    "                    marker_color='rgba(102, 126, 234, 0.7)',\n",
    "                    hovertemplate='<b>Charges Range:</b> $%{x}<br><b>Count:</b> %{y}<extra></extra>'),\n",
    "        row=1, col=1\n",
    "    )\n",
    "    \n",
    "    # Age distribution\n",
    "    fig.add_trace(\n",
    "        go.Histogram(x=df['age'], nbinsx=20, name='Age',\n",
    "                    marker_color='rgba(255, 107, 107, 0.7)',\n",
    "                    hovertemplate='<b>Age:</b> %{x}<br><b>Count:</b> %{y}<extra></extra>'),\n",
    "        row=1, col=2\n",
    "    )\n",
    "    \n",
    "    # BMI distribution\n",
    "    fig.add_trace(\n",
    "        go.Histogram(x=df['bmi'], nbinsx=25, name='BMI',\n",
    "                    marker_color='rgba(78, 205, 196, 0.7)',\n",
    "                    hovertemplate='<b>BMI:</b> %{x}<br><b>Count:</b> %{y}<extra></extra>'),\n",
    "        row=2, col=1\n",
    "    )\n",
    "    \n",
    "    # Children distribution\n",
    "    fig.add_trace(\n",
    "        go.Histogram(x=df['children'], nbinsx=6, name='Children',\n",
    "                    marker_color='rgba(255, 230, 102, 0.7)',\n",
    "                    hovertemplate='<b>Children:</b> %{x}<br><b>Count:</b> %{y}<extra></extra>'),\n",
    "        row=2, col=2\n",
    "    )\n",
    "    \n",
    "    fig.update_layout(\n",
    "        title_text=\"üìà Enhanced Distribution Analysis\",\n",
