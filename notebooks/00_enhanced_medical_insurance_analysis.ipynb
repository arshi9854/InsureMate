    "# Enhanced correlation heatmap with modern styling\n",
    "def create_enhanced_correlation_heatmap(df):\n",
    "    # Prepare numerical data\n",
    "    df_encoded = df.copy()\n",
    "    df_encoded['sex'] = df_encoded['sex'].map({'male': 1, 'female': 0})\n",
    "    df_encoded['smoker'] = df_encoded['smoker'].map({'yes': 1, 'no': 0})\n",
    "    df_encoded = pd.get_dummies(df_encoded, columns=['region'], prefix='region')\n",
    "    \n",
    "    # Calculate correlation\n",
    "    corr_matrix = df_encoded.corr()\n",
    "    \n",
    "    # Create enhanced heatmap\n",
    "    fig = go.Figure(data=go.Heatmap(\n",
    "        z=corr_matrix.values,\n",
    "        x=corr_matrix.columns,\n",
    "        y=corr_matrix.columns,\n",
    "        colorscale='RdBu',\n",
    "        zmid=0,\n",
    "        text=np.round(corr_matrix.values, 2),\n",
    "        texttemplate=\"%{text}\",\n",
    "        textfont={\"size\": 10},\n",
    "        hoverongaps=False,\n",
    "        hovertemplate='<b>%{y}</b> vs <b>%{x}</b><br>Correlation: %{z:.3f}<extra></extra>'\n",
    "    ))\n",
    "    \n",
    "    fig.update_layout(\n",
    "        title='üîó Enhanced Correlation Matrix',\n",
    "        title_x=0.5,\n",
    "        title_font_size=18,\n",
    "        width=800,\n",
    "        height=600,\n",
    "        template=\"plotly_white\"\n",
    "    )\n",
    "    \n",
    "    return fig\n",
    "\n",
    "# Display enhanced correlation heatmap\n",
    "corr_fig = create_enhanced_correlation_heatmap(df)\n",
    "corr_fig.show()"
   ]
  },
